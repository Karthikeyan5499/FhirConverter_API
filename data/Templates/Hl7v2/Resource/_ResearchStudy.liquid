{
    "fullUrl":"urn:uuid:{{ ID }}",
    "resource":{
        "resourceType": "ResearchStudy",
        "id":"{{ ID }}",
        "meta": {
            {% include 'DataType/Meta'  SourceSystemName: SourceSystemName SourceSystemPath: SourceSystemPath HL7MessageCode: HL7MessageCode ExtensionIdentifierValue: ExtensionIdentifierValue %}
        },
        
        {% if CTI -%}
            "status":"active",
        {% endif -%}
      
       "identifier" : [
        { 
            {% include 'DataType/EIIdentifier' EI: CTI.1 -%} 
        },
       ], 

       "phase" : { 
            {% include 'DataType/CWECodeableConcept' CWE: CTI.2 -%} 
        },
    },
    "request":{
        "method":"PUT",
        "url":"ResearchStudy/{{ ID }}",
    },
},
