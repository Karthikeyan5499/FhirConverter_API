{
    "fullUrl": "urn:uuid:{{ ID }}",
    "resource": {
        "resourceType": "DocumentReference",
        "id": "{{ ID }}",

        "status": "current",
        "docStatus": "preliminary",

        "type": {
            "coding": [
                {
                    "system": "http://example.com/v2-to-fhir/CodeSystem/document-type",
                    "code": "FULL-HL7",
                    "display": "Full Original HL7 Message"
                }
            ],
            "text": "Original HL7 Message"
        },

        "content": [
            {
                "attachment": {
                    "contentType": "text/plain",
                    "data": "{{ hl7Base64 }}",
                    "title": "Full HL7 Message"
                }
            }
        ],
        "meta": {
            {% include 'DataType/meta'  SourceSystemName: SourceSystemPath SourceSystemCode: SourceSystemName HL7MessageCode: HL7MessageCode ExtensionIdentifierValue: ExtensionIdentifierValue %}
        }
    }
}