{
    "fullUrl":"urn:uuid:{{ ID }}",
    "resource":{
        "resourceType": "Bundle",
        "id":"{{ ID }}",
        {% if MSH.7 -%}
            "timestamp":"{{ MSH.7.Value | format_as_date_time }}",
        {% endif -%}
        {% comment -%} Placeholder provided for customization by users. If left empty, it will be removed in post-processing. {% endcomment -%}
        "identifier":
        {
           "value":"{{ MSH.10.Value }}",
        },
        "meta": {
            {% include 'DataType/meta' lastUpdated: LastUpdatedVariable SourceSystemName: SourceSystemName SourceSystemPath: SourceSystemPath HL7MessageCode: HL7MessageCode ExtensionIdentifierValue: ExtensionIdentifierValue %}
        },
        {% if MSH.10 -%}
            "type":"message",
        {% endif -%}
    },
    "request":{
        "method":"PUT",
        "url":"Bundle/{{ ID }}",
    },
},
